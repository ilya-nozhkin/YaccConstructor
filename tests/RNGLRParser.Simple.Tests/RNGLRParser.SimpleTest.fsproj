<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
  </PropertyGroup>
  
  <Import Project="..\..\src\YaccConstructor\YaccConstructor.targets" />
  
  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <CallTarget Targets="CallYC" />
  </Target>
  
  <ItemGroup>	
    <YC Include="./*.yrd" OutputFile="%(Filename).yrd.fs" Generator="RNGLRGenerator -pos int -token int -module RNGLR.Parse%(Filename) -table LR -translate true" />
    <Compile Include="LexCommon.fs" />
    <Compile Include="Attrs.yrd.fs" />
    <Compile Include="_Brackets.yrd.fs" />
    <Compile Include="Calc.yrd.fs" />
    <Compile Include="Cond.yrd.fs" />
    <Compile Include="Counter.yrd.fs" />
    <Compile Include="Cycle.yrd.fs" />
    <Compile Include="Epsilon.yrd.fs" />
    <Compile Include="InfEpsilon.yrd.fs" />
    <Compile Include="List.yrd.fs" />
    <Compile Include="LolCalc.yrd.fs" />
    <Compile Include="LongCycle.yrd.fs" />
    <Compile Include="Longest.yrd.fs" />
    <Compile Include="Order.yrd.fs" />
    <Compile Include="Resolvers.yrd.fs" />
    <Compile Include="Omit.yrd.fs" />
    <Compile Include="Eps.yrd.fs" />
    <Compile Include="Eps2.yrd.fs" />
    <Compile Include="ListEps.yrd.fs" />
    <Compile Include="RNGLRParserSimpleTest.fs" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\..\src\RNGLRAbstractParser\RNGLRAbstractParser.fsproj" />
    <ProjectReference Include="..\..\src\RNGLRCommon\RNGLRCommon.fsproj" />
    <ProjectReference Include="..\..\src\RNGLRGenerator\RNGLRGenerator.fsproj" />
    <ProjectReference Include="..\..\src\RNGLRParser\RNGLRParser.fsproj" />
    <ProjectReference Include="..\..\src\YaccConstructor\YaccConstructor.fsproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="15.8.0" />
    <PackageReference Include="NUnit" Version="3.10.1" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.10.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="4.5.0" />
    <PackageReference Update="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>
</Project>